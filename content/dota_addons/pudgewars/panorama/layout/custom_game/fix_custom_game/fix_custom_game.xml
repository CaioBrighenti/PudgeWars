<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c"/>
		<include src="file://{resources}/layout/custom_game/fix_custom_game/fix_custom_game.css"/>
	</styles>

	<scripts>
		<include src="file://{resources}/layout/custom_game/fix_custom_game/fix_custom_game.js"/>
	</scripts>

	<Panel class="FixCG_Root" hittest="false">
		<Image id="FixCG_Image" src="file://{resources}/layout/custom_game/fix_custom_game/fix_custom_game_bg.png"/>
		<Panel id="FixCG_MainText">
			<Label id="FixCG_Header" hittest="false" text="#fix_game_header"/>
			<Label id="FixCG_Description" hittest="false" text="#fix_game_description"/>
		</Panel>
		<Button class="FixCG_Button" id="FixCG_More" onactivate="ExternalBrowserGoToURL('https://fcalife.github.io/fixthelobbies/')">
			<Label class="FixCH_ButtonText" text="#fix_game_more"/>
		</Button>
		<Button class="FixCG_Button" id="FixCG_Dismiss" onactivate="CloseFixGame()">
			<Label class="FixCH_ButtonText" text="#fix_game_dismiss"/>
		</Button>
	</Panel>
</root>
